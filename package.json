{
  "name": "mobile",
  "version": "1.0.0",
  "scripts": {
    "dev": "APP_ENV=localhost expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "gen": "graphql-codegen --config codegen.yml",
    "build": "expo build:ios ",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "submit:ios": "eas submit -p ios --latest",
    "submit:android": "eas submit -p android --latest",
    "lint": "eslint \"./**/*.{ts,tsx}\" --fix",
    "deploy:ios": "eas build --platform ios --auto-submit",
    "deploy:android": "eas build --platform android --auto-submit",
    "start": "expo start --dev-client",
    "build-for-dev:ios": "eas build --profile development --platform ios",
    "build-for-dev:android": "eas build --profile development --platform android",
    "dev-the-build:ios": "yarn dev --dev-client",
    "test": "jest --watch"
  },
  "dependencies": {
